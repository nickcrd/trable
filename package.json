{
  "name": "trable",
  "version": "0.1.0",
  "description": "Trable is an indoor localization app based on Bluetooth Low Energy",
  "author": "nickcrd",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickcrd/trable.git"
  },
  "bin": {
    "trable-cli": "./cli.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check && npm run build:js",
    "build:js": "babel src --out-dir out --extensions \".ts,.tsx\" --source-maps inline",
    "start": "node ./out/app.js",
    "test": "mocha --recursive \"tests\""
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "celebrate": "^12.1.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "joi": "^17.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.19",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "typescript": "^3.9.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.27",
    "@types/morgan": "^1.9.1",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "mocha": "^8.0.1"
  },
  "license": "MIT"
}
